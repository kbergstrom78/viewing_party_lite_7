<%= link_to 'Home', root_path %>
<h1>Viewing Party</h1>
<% if current_user %>
  <%= link_to "Log Out", logout_path, method: :delete %>

  <h2>Existing Users:</h2>
  <ul>
    <% @users.each do |user| %>
      <li><%= user.email %></li> <!-- Here we replaced the link to show page with just the email address -->
    <% end %>
  </ul>
<% else %>
  <%= link_to "Log In", login_path %>
  <%= link_to "Create an Account", new_user_path %>
<% end %>
